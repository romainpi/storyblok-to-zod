{
  "name": "storyblok-to-zod",
  "type": "module",
  "version": "0.0.1",
  "description": "Generate a Zod schema from your components from a Storyblok space",
  "main": "./dist/index.mjs",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "dev": "pnpm run build && node dist/index.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "storyblok",
    "cli",
    "node",
    "javascript"
  ],
  "author": "Romain Pironneau",
  "license": "ISC",
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1"
  },
  "dependencies": {
    "chalk": "^5.6.2"
  }
}
